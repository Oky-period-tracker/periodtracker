{
  "name": "@oky/api",
  "version": "0.0.1",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc && yarn run copy-static-assets",
    "clean": "shx rm -rf dist/*",
    "compile": "yarn run build",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "start": "yarn run serve",
    "serve": "NODE_PATH=./dist node dist/index.js",
    "test": "NODE_PATH=./src jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch": "nodemon --legacy-watch --watch src -e ts,ejs --exec NODE_PATH=./src ts-node ./src/index",
    "watch-test": "yarn run test -- --watchAll",
    "rm": "shx rm -rf node_modules"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "^1.5.3",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.5.0",
    "@types/shelljs": "^0.8.15",
    "@types/uuid": "10.0.0",
    "concurrently": "^8.2.2",
    "jest": "29.7.0",
    "nodemon": "^3.1.4",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "ts-jest": "29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@oky/core": "*",
    "bcrypt": "^5.1.1",
    "body-parse": "^0.1.0",
    "class-validator": "0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "emoji-regex": "^10.3.0",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typeorm-typedi-extensions": "^0.4.1",
    "typescript": "^5.5.4",
    "uuid": "10.0.0"
  }
}
