{
  "name": "@oky/mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "clean": "cd .",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "compile": "tsc -b",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "remotedev": "remotedev --hostname=0.0.0.0 --port=8000",
    "start": "react-native start --projectRoot=../../",
    "test": "cd .",
    "rm": "cd ios && shx rm -rf Pods",
    "deploy:android": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd .."
  },
  "dependencies": {
