{
  "expo": {
    "name": "Oky",
    "slug": "oky",
    "version": "8.0.0",
    "versionCode": 80,
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.unicef.oky",
      "permissions": ["INTERNET"]
    },
    "android": {
      "package": "com.unicef.oky",
      "googleServicesFile": "./google-services.json",
      "versionCode": 80,
      "versionName": "8.0.0",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["INTERNET"],
      "keystore": {
        "keystorePath": "/Users/dev/keys/oky.keystore",
        "keystorePassword": "tbox123",
        "keyAlias": "oky",
        "keyPassword": "tbox123"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1d358414-c33c-4a76-9ca1-5857924cd304"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ]
  }
}
