{
  "name": "@oky/oky-cms",
  "version": "0.0.1",
  "main": "dist/index.js",
  "private": true,
  "description": "Period tracker CMS, with Express, typescript, TypeORM and ejs",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express-session": "^1.18.0",
    "@types/jest": "29.5.12",
    "@types/jquery": "^3.5.30",
    "@types/node": "^22.5.0",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "jest": "29.7.0",
    "ts-jest": "29.2.4",
    "ts-node": "10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@oky/core": "*",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "firebase": "^10.13.0",
    "firebase-admin": "12.3.1",
    "i18n": "^0.15.1",
    "nodemon": "^3.1.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "0.3.20",
    "typescript": "^5.5.4",
    "uuid": "10.0.0",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "build": "tsc && yarn run copy-static-assets",
    "rm": "shx rm -rf node_modules",
    "clean": "shx rm -rf dist/*",
    "compile": "yarn run build",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "serve": "NODE_PATH=./dist node dist/src/index.js",
    "start": "nodemon --legacy-watch -e ts --exec ts-node --files src/index",
    "migrate-up": "ts-node ../../node_modules/typeorm/cli -f /ormconfig.ts migration:run",
    "projections": "ts-node ./src/projections/stale-data-projection.ts",
    "test": "NODE_PATH=./src jest --forceExit --coverage --verbose --detectOpenHandles"
  }
}
