    "jest": "29.7.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-devtools": "4.24.7",
    "react-test-renderer": "17.0.2",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-server": "^0.3.1"
