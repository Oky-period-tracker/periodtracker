{
  "name": "@oky/api",
  "version": "0.0.1",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc && yarn run copy-static-assets",
    "clean": "shx rm -rf dist/*",
    "compile": "yarn run build",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "format": "prettier --write '{.,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "start": "yarn run serve",
    "serve": "NODE_PATH=./dist node dist/index.js",
    "test": "NODE_PATH=./src jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch": "nodemon --legacy-watch --watch src -e ts,ejs --exec NODE_PATH=./src ts-node ./src/index",
    "watch-test": "yarn run test -- --watchAll",
    "nuke": "shx rm -rf node_modules"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.6",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.2",
    "@types/shelljs": "^0.8.10",
    "@types/uuid": "9.0.0",
    "concurrently": "^6.5.1",
    "jest": "29.7.0",
    "nodemon": "^2.0.15",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "ts-jest": "29.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@oky/core": "*",
    "cors": "^2.8.5",
    "bcrypt": "^5.0.1",
    "body-parse": "^0.1.0",
    "class-validator": "0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.7",
    "emoji-regex": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.7.7",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.41",
    "typeorm-typedi-extensions": "^0.2.3",
    "uuid": "9.0.0"
  }
}
